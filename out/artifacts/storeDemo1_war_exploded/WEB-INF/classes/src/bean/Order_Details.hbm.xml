<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="bean.Order_Details" table="order_details">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
			<property name="goods" column="goods"></property>
    		<property name="order_number" column="order_number"></property>
			<property name="price" column="price"></property>
			<property name="number" column="number"></property>
			<property name="total_number" column="total_number"></property>
    	</class>
    </hibernate-mapping>