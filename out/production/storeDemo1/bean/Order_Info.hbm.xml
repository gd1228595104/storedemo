<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="bean.Order_Info" table="order_info">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
    		<property name="order_number" column="order_number"></property>
			<property name="address" column="address"></property>
			<property name="affiliated_customers" column="affiliated_customers"></property>
			<property name="phone" column="phone"></property>
			<property name="total_price" column="total_price"></property>
			<property name="order_time" column="order_time"></property>
			<property name="status" column="status"></property>
    	</class>
    </hibernate-mapping>